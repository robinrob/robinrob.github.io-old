  %body.detached
    #toolbar
      .toolbar-item.close-left
        %button#close-button-left
      .toolbar-item.flexable-space
      #search-results-matches.toolbar-item.hidden
      .toolbar-item
        %input#search{:incremental => "", :results => "0", :type => "search"}/
        #search-toolbar-label.toolbar-label
      .toolbar-item.close-right
        %button#close-button-right
    #main
      #main-panels{:spellcheck => "false"}
      #main-status-bar.status-bar
        #anchored-status-bar-items
          %button#dock-status-bar-item.status-bar-item
            .glyph
            .glyph.shadow
          %button#console-status-bar-item.status-bar-item
            .glyph
            .glyph.shadow
          #counters
            #error-warning-count.hidden
    #drawer
      #console-view
        #console-messages.monospace
          #console-prompt{:spellcheck => "false"}
            %br/
      #drawer-status-bar.status-bar
        #other-drawer-status-bar-items
          %button#clear-console-status-bar-item.status-bar-item.clear-status-bar-item
            .glyph
            .glyph.shadow
          #console-filter.scope-bar.status-bar-item
