/ Footer
%footer
  .container
    .row
      .col-lg-8.col-lg-offset-2.col-md-10.col-md-offset-1
        %ul.list-inline.text-center
          %li
            %a{:href => "https://github.com/{{ site.github }}", :target => "_blank"}
              %span Github page of {{ site.author }}
              %span.fa-stack.fa-lg
                %i.fa.fa-circle.fa-stack-2x
                %i.fa.fa-github.fa-stack-1x.fa-inverse
          %li
            %a{:href => "https://www.linkedin.com/in/{{ site.linkedin }}", :target => "_blank"}
              %span LinkedIn profile page of {{ site.author }}
              %span.fa-stack.fa-lg
                %i.fa.fa-circle.fa-stack-2x
                %i.fa.fa-linkedin.fa-stack-1x.fa-inverse
          %li
            %a{:href => "mailto:robin@rsmith.io", :target => "_blank"}
              %span Send email to {{ site.author }}
              %span.fa-stack.fa-lg
                %i.fa.fa-circle.fa-stack-2x
                %i.fa.fa-envelope.fa-stack-1x.fa-inverse

        %p.copyright.text-muted Copyright Â© {{ site.brand }} 2015

